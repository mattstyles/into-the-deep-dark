{
  "name": "@itdd/app",
  "private": true,
  "version": "0.0.0",
  "description": "Into the deep dark",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_PATH=src NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "NODE_PATH=src DEBUG=true parcel src/index.html"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt.styles/into-the-deep-dark",
    "directory": "packages/app"
  },
  "keywords": [
    "itdd",
    "into the deep dark"
  ],
  "author": {
    "name": "Matt Styles",
    "url": "https://github.com/mattstyles"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstyles/into-the-deep-dark/issues"
  },
  "homepage": "https://github.com/mattstyles/into-the-deep-dark",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-react-require": "^1.0.1",
    "create-raid-app": "^6.0.0",
    "gh-pages": "^2.2.0",
    "mkdirp": "^1.0.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "rimraf": "^3.0.2",
    "standard": "^14.3.3"
  },
  "dependencies": {
    "@raid/addons": "^6.0.0",
    "@raid/hooks": "^6.0.0",
    "@raid/navigator": "^6.0.0",
    "@raid/streams": "^6.0.0",
    "@styled-system/css": "5.1.5",
    "@styled-system/theme-get": "5.1.2",
    "@styled-system/variant": "5.1.5",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.15",
    "most": "^1.8.0",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "raid": "^6.0.0",
    "react-helmet": "^5.2.1",
    "reselect": "4.0.0",
    "styled-system": "5.1.5"
  }
}
