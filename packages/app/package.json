{
  "name": "into-the-deep-dark",
  "private": true,
  "version": "0.0.0",
  "description": "@TODO description",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "standard src spec",
    "test": "NODE_PATH=src tape -r babel-register -r ./spec/setup spec/*.test.js",
    "bench": "node -r @std/esm bench/collections.js",
    "prebuild": "npm run clean",
    "build": "NODE_PATH=src NODE_ENV=production browserify --extension jsx src/main.js | uglifyjs -cm > dist/main.js && ncp src/index.html dist/index.html && ncp src/assets dist/assets",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "NODE_PATH=src DEBUG=true bin/start"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "babel": {
    "presets": [
      "urban"
    ],
    "plugins": [
      "transform-react-require",
      "lodash",
      "styled-jsx/babel"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/into-the-deep-dark.git"
  },
  "keywords": [
    "raid",
    "react",
    "deepest",
    "darkest",
    "game",
    "deep",
    "dark"
  ],
  "author": "mattstyles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstyles/into-the-deep-dark/issues"
  },
  "homepage": "https://mattstyles.github.io/into-the-deep-dark",
  "devDependencies": {
    "@idd/components": "*",
    "@std/esm": "^0.12.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-urban": "^1.0.0",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.4",
    "browserify": "^14.4.0",
    "budo": "^9.4.7",
    "create-raid-app": "2.10.0",
    "envify": "^4.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "gh-pages": "^1.0.0",
    "jsdom": "^11.3.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "standard": "^10.0.3",
    "styled-jsx": "^2.0.0",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "eventemitter3": "^2.0.3",
    "lodash": "^4.17.4",
    "most": "^1.7.2",
    "open-color": "^1.5.1",
    "prop-types": "^15.5.10",
    "raf-stream": "^3.0.0",
    "raid": "^2.10.0",
    "raid-addons": "^2.10.0",
    "raid-fl": "^2.11.0",
    "raid-navigator": "^2.12.0",
    "raid-streams": "^2.10.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^2.5.0",
    "remarkable": "^1.7.1",
    "reselect": "^2.5.4"
  }
}
